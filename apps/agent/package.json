{
  "name": "@deepractice-ai/agent",
  "version": "0.6.4",
  "description": "Deepractice AI Agent - Full-stack application (Server + Web UI + CLI)",
  "type": "module",
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "bin": {
    "agentx": "./dist/cli/bin.js"
  },
  "exports": {
    ".": "./dist/server/index.js",
    "./server": "./dist/server/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm dev:web\" \"pnpm dev:server\"",
    "dev:web": "vite --host --logLevel error",
    "dev:server": "NODE_ENV=development tsx watch server/index.ts",
    "dev:clean": "lsof -ti:5200 | xargs kill -9 2>/dev/null || true && pnpm dev",
    "clean": "rm -rf dist",
    "build": "pnpm clean && pnpm build:web && pnpm build:server",
    "build:web": "vite build",
    "build:server": "tsup && chmod +x dist/cli/bin.js",
    "start": "NODE_ENV=production node dist/server/index.js",
    "prepublishOnly": "pnpm build",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.11.37",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/merge": "^6.11.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.6",
    "@deepractice-ai/agentx-node": "workspace:*",
    "@deepracticex/logger": "^1.1.1",
    "@replit/codemirror-minimap": "^0.5.2",
    "@uiw/react-codemirror": "^4.25.2",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^13.0.0",
    "config": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.24",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.515.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "remark-gfm": "^4.0.1",
    "rxjs": "^7.8.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zustand": "^5.0.8"
  },
  "optionalDependencies": {
    "node-pty": "^1.1.0-beta34"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite": "^7.0.4"
  }
}
