{
  "name": "@deepractice-ai/agentx-event",
  "version": "0.1.0",
  "description": "Industry-standard event system for AI Agent communication",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./base": {
      "types": "./dist/base/index.d.ts",
      "import": "./dist/base/index.js",
      "require": "./dist/base/index.cjs"
    },
    "./l1-stream": {
      "types": "./dist/l1-stream/index.d.ts",
      "import": "./dist/l1-stream/index.js",
      "require": "./dist/l1-stream/index.cjs"
    },
    "./l2-event": {
      "types": "./dist/l2-event/index.d.ts",
      "import": "./dist/l2-event/index.js",
      "require": "./dist/l2-event/index.cjs"
    },
    "./l4-participant": {
      "types": "./dist/l4-participant/index.d.ts",
      "import": "./dist/l4-participant/index.js",
      "require": "./dist/l4-participant/index.cjs"
    },
    "./l5-direction": {
      "types": "./dist/l5-direction/index.d.ts",
      "import": "./dist/l5-direction/index.js",
      "require": "./dist/l5-direction/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@deepractice-ai/agentx-types": "workspace:*",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
