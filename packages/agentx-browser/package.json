{
  "name": "@deepractice-ai/agentx-browser",
  "version": "0.1.0",
  "description": "AgentX SDK for Browser - WebSocket client for Claude Agent",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agentx",
    "sdk",
    "agent",
    "claude",
    "anthropic",
    "ai",
    "llm",
    "browser",
    "websocket"
  ],
  "author": "Deepractice AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/Agent.git"
  },
  "bugs": {
    "url": "https://github.com/Deepractice/Agent/issues"
  },
  "homepage": "https://github.com/Deepractice/Agent#readme",
  "dependencies": {
    "@deepractice-ai/agentx-api": "workspace:*",
    "@deepractice-ai/agentx-core": "workspace:*",
    "@deepractice-ai/agentx-types": "workspace:*",
    "consola": "^3.2.3",
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
