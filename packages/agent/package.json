{
  "name": "@agentxjs/agent",
  "version": "1.1.4",
  "description": "Agent runtime for AgentX ecosystem - Agent lifecycle and event management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ai",
    "agent",
    "runtime",
    "lifecycle",
    "agentx",
    "deepractice"
  ],
  "author": "Deepractice AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/Agent.git"
  },
  "bugs": {
    "url": "https://github.com/Deepractice/Agent/issues"
  },
  "homepage": "https://github.com/Deepractice/Agent#readme",
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/types": "workspace:*",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@deepracticex/vitest-cucumber": "^1.4.3",
    "@types/node": "^24.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.3"
  }
}
