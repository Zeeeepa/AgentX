{
  "name": "@agentxjs/remote-ecosystem",
  "version": "0.1.0",
  "description": "Remote Ecosystem for AgentX - Network-backed Ecosystem factory",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/AgentX",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/network": "workspace:*",
    "@agentxjs/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
