{
  "name": "@deepractice-ai/agentx-node",
  "version": "0.1.0",
  "description": "Node.js Runtime for AgentX with Claude driver",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "@deepractice-ai/agentx-agent": "workspace:*",
    "@deepractice-ai/agentx-common": "workspace:*",
    "@deepractice-ai/agentx-engine": "workspace:*",
    "@deepractice-ai/agentx": "workspace:*",
    "@deepractice-ai/agentx-types": "workspace:*",
    "better-sqlite3": "^11.7.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
