{
  "name": "@agentxjs/common",
  "version": "1.4.0",
  "description": "Common utilities for AgentX platform - Logger facade and shared infrastructure",
  "keywords": [
    "agentx",
    "common",
    "logger",
    "internal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deepractice/Agent",
    "directory": "packages/agentx-common"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentxjs/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
