{
  "name": "agentxjs",
  "version": "0.1.0",
  "description": "Unified API for AI Agents - Server and Browser",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/AgentX",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist test-results",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/mirror": "workspace:*",
    "@agentxjs/network": "workspace:*",
    "@agentxjs/types": "workspace:*"
  },
  "peerDependencies": {
    "@agentxjs/persistence": "workspace:*",
    "@agentxjs/runtime": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@agentxjs/persistence": {
      "optional": true
    },
    "@agentxjs/runtime": {
      "optional": true
    }
  },
  "devDependencies": {
    "@agentxjs/persistence": "workspace:*",
    "@agentxjs/runtime": "workspace:*",
    "@types/node": "^24.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
