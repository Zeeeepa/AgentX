{
  "name": "@agentxjs/server",
  "version": "1.9.10-dev",
  "description": "AgentX Server - WebSocket server with Platform support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentx-server": "./bin/server.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "bun run bin/server.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@agentxjs/core": "workspace:*",
    "@agentxjs/node-platform": "workspace:*",
    "@agentxjs/mono-driver": "workspace:*",
    "commonxjs": "^0.1.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
